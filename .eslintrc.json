{
  "extends": [
    "airbnb-base"
  ],
  "plugins": [],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
    "no-tabs": 0,
    "max-len": [
      1,
      180,
      4
    ],
    "no-mixed-spaces-and-tabs": 0,
    "no-underscore-dangle": 0,
    "func-names": 0,
    "linebreak-style": 0,
    "no-use-before-define": 0,
    "eol-last": 0
  }
}